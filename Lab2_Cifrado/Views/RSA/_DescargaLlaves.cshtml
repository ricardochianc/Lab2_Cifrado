@model Lab2_Cifrado.Models.Serie3.RSA

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">2. Seleccione cifrado/descifrado:</h3>
    </div>
    <div class="panel-body">

        @using (Html.BeginForm("ResultadoRSA", "RSA"))
        {
            <button type="submit" class="btn btn-default" name="DescargarPublica" value="DescargarPublica">Descargar llave pública</button>
            <button type="submit" class="btn btn-default" name="DescargarPrivada" value="DescargarPrivada">Descargar llave privada</button>
            <button type="submit" class="btn btn-default" name="ContinuarRSA" value="ContinuarRSA">Continuar</button>
        }
    </div>
</div>
