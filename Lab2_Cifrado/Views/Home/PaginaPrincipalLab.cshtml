
@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br/>
<h3>Home</h3>
<blockquote class="col-md-5">
    <small>Serie 1: Zig-Zag, Espiral, César</small>
    <small>Serie 2: S-DES</small>
    <small>Serie 3: RSA</small>
</blockquote>

<div class="col-md-7">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">1. Seleccione serie:</h3>
        </div>
        <div class="panel-body">

            @using (Html.BeginForm("PaginaPrincipalLab", "Home"))
            {
                <button type="submit" class="btn btn-default" name="Serie1" value="Serie1">Serie 1</button>
                <button type="submit" class="btn btn-default" name="Serie2" value="Serie2">Serie 2</button>
                <button type="submit" class="btn btn-default" name="Serie3" value="Serie3">Serie 3</button>
            }
        </div>
    </div>
</div>

