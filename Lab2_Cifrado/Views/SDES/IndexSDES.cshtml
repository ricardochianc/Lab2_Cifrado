@using Lab2_Cifrado.Instancia
@model Lab2_Cifrado.Models.Serie2.SDES

@{
    ViewBag.Title = "S-DES | ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h2>S-DES</h2>

@if (Data.Instancia.ModificarPermutaciones)
{
    @Html.Partial("_CargarArchivoPermutaciones")
}

@if (Data.Instancia.ArchivoCargado == false && Data.Instancia.ModificarPermutaciones == false)
{
    @Html.Partial("_CargarArchivoSDES")
}

@if (Data.Instancia.ArchivoCargado && Data.Instancia.EleccionOperacion == false)
{
    @Html.Partial("_OperarSDES")
}

@if (Data.Instancia.ArchivoCargado && Data.Instancia.EleccionOperacion)
{
    @Html.Partial("_ResultadoSDES")
}

