@using Lab2_Cifrado.Instancia
@model Lab2_Cifrado.Models.Serie3.RSA

@{
    ViewBag.Title = "Serie 3 |";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br/>
<h2>Serie 3</h2>

<div class="col-md-7">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">2. Configuración RSA:</h3>
        </div>
        <div class="panel-body">

            @using (Html.BeginForm("SeleccionarOpcionSerie3", "Serie3"))
            {
                <button type="submit" class="btn btn-default" name="GenerarLlaves" value="GenerarLllaves">Generar Llaves</button>
                <button type="submit" class="btn btn-default" name="Continuar" value="Continuar">Continuar, ya poseo llave para utilizar algoritmo RSA</button>
            }
        </div>
    </div>
</div>